{
  "name": "tmdict",
  "version": "3.5.0",
  "scripts": {
    "build": "cross-env APP=all NODE_ENV=production run-s clean compile pack",
    "build:dev": "cross-env APP=all NODE_ENV=development run-s clean compile pack:all",
    "build:chaldea": "cross-env APP=chaldea NODE_ENV=production run-s clean compile pack:chaldea",
    "clean": "rimraf bin dist src/**/__tmp",
    "compile": "tsc && node bin/main.js",
    "dev": "run-s build:dev start",
    "dev:book": "cross-env APP=book NODE_ENV=development run-s clean compile watch:book",
    "dev:chaldea": "cross-env APP=chaldea NODE_ENV=development run-s clean compile watch:chaldea",
    "dev:tmdict": "cross-env APP=tmdict NODE_ENV=development run-s clean compile watch:tmdict",
    "lint": "run-p lint:ts",
    "lint:ts": "eslint src/**/*.ts; exit 0",
    "pack": "run-p pack:tmdict pack:book",
    "pack:all": "run-p pack:chaldea pack:tmdict pack:book",
    "pack:book": "rollup -c src/app/book/rollup.config.js",
    "pack:chaldea": "rollup -c src/app/chaldea/rollup.config.js",
    "pack:tmdict": "rollup -c src/app/tmdict/rollup.config.js",
    "start": "sirv dist",
    "watch:book": "rollup -c src/app/book/rollup.config.js -w",
    "watch:chaldea": "rollup -c src/app/chaldea/rollup.config.js -w",
    "watch:tmdict": "rollup -c src/app/tmdict/rollup.config.js -w"
  },
  "dependencies": {
    "page": "^1.11.6",
    "tiny-swiper": "^2.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.178",
    "@types/marked": "^4.0.2",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "fs-extra": "^10.0.0",
    "fuse.js": "^6.6.2",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.7",
    "jimp": "^0.16.0",
    "lodash": "^4.17.21",
    "marked": "^4.0.12",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.49.8",
    "sirv-cli": "^2.0.2",
    "sitemap": "^7.1.1",
    "svelte": "^3.46.4",
    "typescript": "^4.5.5"
  }
}
